c376b57530af"],

Cell[TextData[{
 "Harmonic oscillator with potential in form: \n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["V", "TI"], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["x", "TI"], ",", 
             StyleBox["y", "TI"]}], ")"}], "\[LongEqual]", 
         FractionBox["1", "2"], 
         StyleBox["m", "TI"], 
         SubsuperscriptBox["\[Omega]", 
          StyleBox["x", "TI"], "2"], 
         SuperscriptBox[
          StyleBox["x", "TI"], "2"], "+", 
         FractionBox["1", "2"], 
         StyleBox["m", "TI"], 
         SubsuperscriptBox["\[Omega]", 
          StyleBox["y", "TI"], "2"], 
         SuperscriptBox[
          StyleBox["y", "TI"], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "V(x,y) = \\frac{1}{2}m \\omega_x^2 x^2+\\frac{1}{2}m \\omega_y^2 y^2", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ca372c48-6089-4276-9822-235b1ad62e49"],
 "\n\nExact energies correspond to the following formula: \n\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["E", "TI"], 
          RowBox[{
            SubscriptBox[
             StyleBox["n", "TI"], 
             StyleBox["x", "TI"]], ",", 
            SubscriptBox[
             StyleBox["n", "TI"], 
             StyleBox["y", "TI"]]}]], "\[LongEqual]"}], TraditionalForm], 
     "errors" -> {}, "input" -> "E_{n_x,n_y}  =", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "eae22e28-d246-4d71-96e6-36363edd03a7"],
 "\[HBar]",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[Omega]", 
         RowBox[{"(", 
           RowBox[{
             SubscriptBox[
              StyleBox["n", "TI"], 
              StyleBox["x", "TI"]], "+", 
             SubscriptBox[
              StyleBox["n", "TI"], 
              StyleBox["y", "TI"]], "+", "1"}], ")"}], ",  ", 
         SubscriptBox[
          StyleBox["n", "TI"], 
          StyleBox["x", "TI"]], ",", 
         SubscriptBox[
          StyleBox["n", "TI"], 
          StyleBox["y", "TI"]], "\[LongEqual]", "0", ",", "1", ",", "2", ",", 
         "\[Ellipsis]"}], TraditionalForm], "errors" -> {}, "input" -> 
     "\\omega\\left(n_x + n_y+ 1\\right), \\quad n_x,n_y=0,1,2, \\dots", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f714744b-7090-4d84-baa2-1e8fdc973f9d"]
}], "Text",
